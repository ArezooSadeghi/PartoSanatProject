<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.partosanatproject">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_partosanat_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_partosanat_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.PartoSanatProject"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".ui.activity.CaseContainerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.ServerDataContainerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.CaseTypeContainerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.MenuContainerActivity"
            android:exported="false" />
        <activity
            android:name=".ui.activity.LoginContainerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>